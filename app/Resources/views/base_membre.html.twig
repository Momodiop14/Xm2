<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}NagnDem{% endblock %}</title>
          {% include 'BaseStylesheetsScripts.html.twig'  %}

           {% block style%}
               <link href="https://cdn.datatables.net/1.10.9/css/dataTables.bootstrap.min.css" rel="stylesheet" type="text/css" >



             <style type="text/css">
             .breadcrumb {background: rgba(245, 245, 245, 0.03); border: 0px solid rgba(245, 245, 245, 1); border-radius: 4px; display: block;}
              .breadcrumb li {font-size: 14px;}
              .breadcrumb a {color: rgba(66, 139, 202, 1); text-decoration: none;}
              .breadcrumb a:hover {color: rgba(42, 100, 150, 1);}
              .breadcrumb>.active {color: rgba(31, 27, 27, 1);}
              .breadcrumb>li+li:before {color: rgba(33, 38, 181, 1); content: "\2771\00a0";}

             </style>
               
           {% endblock %}

             {% block script  %}
                   
                                
                  <script src="https://cdn.datatables.net/1.10.9/js/jquery.dataTables.min.js"></script>
                   <script src="https://cdn.datatables.net/1.10.9/js/dataTables.bootstrap.min.js"></script>

           {% endblock %}


        
    </head>
    <body >
         
        <div class="container-fluid" style="background-color:#FDFDFD" >


             {% block header %}
               
                 <div class="header" id="top">                                               
                      {% include 'menu_membre.html.twig'  %}
                   
                  </div>

             {% endblock header %}

             <br><br><br><br>
            
             

             <div> <a href="#top" class='col-sm-offset-11 btn btn-info ancre'> <span class="fa fa-arrow-up"></span> </a>  </div>
             {% block content %}
                 <div class="row">
                   <div class="col-sm-9">
                       <div class="row presentation jumbotron">
                              
                               {% for flashMessage in app.session.flashbag.get('loginSuccess') %}
                                   
                                   <div class="alert alert-info alert-dismissible" role="alert">
                                        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                                          {{ flashMessage }}
                                    </div>
                              {% endfor %}
                              

                              presentation

                              </div>

                              <div class="row">
                               
                               <div class="jumbotron col-lg-6">
                                   temoignage

                               </div>

                              

                               <div class="col-md-offset-2 jumbotron col-lg-3">
                                   top villes plus visitées

                               </div>
                            

                              </div>
                          
                              <div class="row">
                                
                                   <div class="jumbotron col-lg-5">
                                       recherche rapide

                                   </div>

                                  

                                   <div class="col-md-offset-2 jumbotron col-lg-4">
                                       bons plans récents

                                   </div>
                            </div>
                    
                   </div>

                   <div class="col-sm-3"  >
                                                                              
                                   <div class=" jumbotron ">
                                       fil d'actualité

                                   </div>
                             
                  </div>
                </div>
             {% endblock content %}


         
          {# {% include 'BaseFooterScroller.html.twig' %}                #}
        </div>
        
        
    </body>
</html>